#include <cstdlib>
#include <iostream>

using namespace std;
long long q,z,a[100000],i,j,k,l,m,n;
char s[1000000];
int main(int argc, char *argv[])
{gets(s);

  l=0;
  for(i=0;s[i]!='\0';i++)
      l++;
      k=l;

      l=1; j=1;
      for(i=1;i<=k;i=i+l){

       if(n>=5)
       l++;
       n=s[i]-48;
          j++;

      }
      cout<<j-1<<endl;




  return EXIT_SUCCESS;
}